		"regexp",
		"is",
		"regular expression",
		"regular",
		"expression"
	],
	"dependencies": {},
	"devDependencies": {
		"tape": "^4.0.2",
		"covert": "^1.1.0",
		"jscs": "^2.0.0",
		"editorconfig-tools": "^0.1.1",
		"nsp": "^1.0.3",
		"eslint": "^1.0.0",
		"@ljharb/eslint-config": "^1.0.4",
		"semver": "^5.0.1",
